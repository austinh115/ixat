<?xml version="1.0"?>
<!DOCTYPE cross-domain-policy SYSTEM "http://www.adobe.com/xml/dtds/cross-domain-policy.dtd">
<cross-domain-policy>
	<site-control permitted-cross-domain-policies="all"/>
	<allow-access-from domain="ixat.io" to-ports="*" />
	<allow-access-from domain="*.ixat.io" to-ports="*" />
	<allow-http-request-headers-from domain="ixat.io" headers="*" />
	<allow-http-request-headers-from domain="*.ixat.io" headers="*" />
</cross-domain-policy>