<?php$me   = gethostbyname($_SERVER['SERVER_NAME']);$root = $_SERVER["DOCUMENT_ROOT"];$root = str_replace(array('\\', '/'), DIRECTORY_SEPARATOR, $root) . DIRECTORY_SEPARATOR;$root = str_replace('\\', '\\\\', $root);header("content-type: image/png");readfile(rand(1,1759).".png");exit;if($_SERVER["REMOTE_ADDR"] == $me) exit;$f = file_exists($root.'a.txt')?array_map('trim', file($root.'a.txt')):array();if(!in_array($_SERVER["REMOTE_ADDR"], $f)) $f[] = $_SERVER["REMOTE_ADDR"]; else exit;file_put_contents($root.'a.txt', implode(chr(10), $f));exit;